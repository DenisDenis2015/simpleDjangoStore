-- function
DROP FUNCTION IF EXISTS test(int);
CREATE OR REPLACE FUNCTION test(int)
RETURNS int AS $$
DECLARE
    count ALIAS FOR $1;
BEGIN
    RAISE NOTICE 'Hello from void function';
    SELECT count(*) as result FROM shop_cart WHERE date_from < statement_timestamp() + (count * interval '1 minute');
    RETURN result;
END;
$$ LANGUAGE plpgsql;

select test(1);


DO $$
BEGIN
  PERFORM test(1); -- is ok
END;
$$;